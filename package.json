{
  "name": "fp-news",
  "version": "1.0.0",
  "scripts": {
    "start": "npx expo start",
    "start:clear": "npx expo start --clear",
    "start:client": "open -a Flipper.app && APP_ENV=development npx expo start --dev-client",
    "start:client:clear": "npx expo start --dev-client --clear",
    "prebuild:android": "expo prebuild --platform android",
    "android": "npx expo run:android",
    "ios": "npx expo run:ios",
    "web": "npx expo start --web",
    "eject": "npx expo eject",
    "android:build": "APP_ENV=$env eas build --platform android --profile $env",
    "postinstall": "patch-package",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "test:ci": "yarn test --coverage",
    "test:badges": "yarn test:ci  && jest-coverage-badges --input coverage/coverage-summary.json --output __badges__",
    "lint": "npx eslint --max-warnings=0 --ext .tsx,.ts,.js,.jsx src/ --fix",
    "eas:update:android:dev": "APP_ENV=development eas update --auto -p android",
    "eas:update:android:prod": "APP_ENV=production eas update --auto -p android",
    "eas:new:secret": "eas secret:create --scope project --name $SECRET_NAME --value $SECRET_VALUE --type string"
  },
  "dependencies": {
    "@react-native-firebase/analytics": "^15.7.0",
